    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@testing-library/preact": "^2.0.1",
    "@types/jest": "^26.0.23",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.0",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss-loader": "^5.2.0",
    "rimraf": "^2.7.1",
    "strip-ansi": "^3.0.1",
