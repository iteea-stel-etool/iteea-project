{
  "name": "preact-custom",
  "version": "1.0.0",
  "description": "Preact Custom Setup",
  "main": "index.ts",
  "scripts": {
    "dev": "webpack serve --mode=development",
    "build": "rimraf ./dist && webpack build --mode=production",
    "test": "jest",
    "update-jest": "jest --updateSnapshot"
  },
  "keywords": [
    "Preact",
    "Typescript",
    "Storybook",
    "Jest"
  ],
